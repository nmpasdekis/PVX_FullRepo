{"Category":"Pages","Access":["SuperAdmin"],"Controller":"[\"$scope\", \"$http\", function ($scope, $http) { \n    $scope.status = \"Stopped\";\n    $scope.Play = function(){\n        $http.post(\"/api/audio/start\").then(r =>{\n            $scope.status = r.data;\n        })\n    }\n    $scope.Restart = function(){\n        $http.post(\"/api/audio/seek\", { timestamp: 0 });\n    }\n}]","Html":"<md-card>\n    <md-toolbar class=\"md-table-toolbar md-default\">\n        <div class=\"md-toolbar-tools\">\n            <h2 class=\"text-center\">Title</h2>\n        </div>\n    </md-toolbar>\n    <md-card-content>\n        <pre>{{CurTime|number:3}}</pre>\n        <pre>{{status}}</pre>\n        <md-button ng-click=\"Play()\">Play</md-button>\n        <md-button ng-click=\"Restart()\">Restart</md-button>\n    </md-card-content>\n    <md-card-actions>\n    \n    </md-card-actions>\n</md-card>","Name":"Player"}